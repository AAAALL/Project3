import pandas as pd

df = pd.read_csv("C:\\Users\\alex0\\OneDrive\\Documenten\\Python Exercises\\UntypedDataSet.csv", sep=",")
print(df.columns)
print(df["HouseholdCharacteristics"].unique())

df_children = df[ ((df["HouseholdCharacteristics"] == '1016090') | df["HouseholdCharacteristics"] == '1017780')]
df_children.head()

if df_children.empty:
    print("No rows match the filtering condition.")
else:
    df_children.groupby("HouseholdCharacteristics")["MedianDevelopmentPurchasingPower_1"].mean().plot(kind="bar")
